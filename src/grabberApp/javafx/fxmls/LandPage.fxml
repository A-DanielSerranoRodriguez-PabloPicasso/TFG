<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grabberApp.javafx.fxmls.ControllerLandPage">
   <children>
      <ScrollPane fitToWidth="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0">
               <children>
                  <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <VBox HBox.hgrow="ALWAYS">
                           <children>
                              <VBox>
                                 <children>
                                    <Label text="Bibliotecas" />
                                    <HBox fx:id="hBoxLibraries" prefHeight="100.0" prefWidth="200.0" />
                                    <ScrollPane minHeight="100.0" vbarPolicy="NEVER">
                                       <content>
                                          <AnchorPane>
                                             <children>
                                                <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                   <children>
                                                      <GridPane fx:id="gpLibraries" hgap="20.0">
                                                         <rowConstraints>
                                                            <RowConstraints vgrow="SOMETIMES" />
                                                         </rowConstraints>
                                                         <padding>
                                                            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                                         </padding>
                                                         <styleClass>
                                                            <String fx:value="topBar" />
                                                            <String fx:value="slightRadius" />
                                                         </styleClass>
                                                      </GridPane>
                                                   </children>
                                                </VBox>
                                             </children>
                                          </AnchorPane>
                                       </content>
                                    </ScrollPane>
                                 </children>
                              </VBox>
                              <VBox prefHeight="231.0" prefWidth="412.0" spacing="20.0">
                                 <children>
                                    <Label text="Videos recientes" />
                                    <HBox fx:id="hbVideos" minHeight="500.0" VBox.vgrow="ALWAYS">
                                       <children>
                                          <ScrollPane fitToHeight="true" hbarPolicy="NEVER" HBox.hgrow="ALWAYS">
                                             <content>
                                                <AnchorPane>
                                                   <children>
                                                      <VBox fx:id="apVideos" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                                   </children>
                                                </AnchorPane>
                                             </content>
                                          </ScrollPane>
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                           <padding>
                              <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
